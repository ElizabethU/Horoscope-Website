<!-- index.erb -->
<!DOCTYPE html>
<head>
  <link href="/css/main.css" media="all" rel="stylesheet">
  <title>Totally Legit Horoscopes.com -- The best in automated internet horoscores</title>
</head>
<body>
    <div class='header'>
        <%= locals[:title] %>
    </div>
    <%= erb :menu %>
    <% @posts.each do |post| %>
      <a href="/blog/<%= post %>"><%= post %></q>
    <% end %>
    <%= yield %>
</body>
</html> 